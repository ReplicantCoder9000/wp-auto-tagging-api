{
  "name": "n8n-nodes-wordpress-auto-tagger",
  "version": "1.0.0",
  "description": "n8n custom node for WordPress Auto-Tagging API integration",
  "keywords": [
    "n8n-community-node-package",
    "wordpress",
    "auto-tagging",
    "ai",
    "content",
    "seo"
  ],
  "license": "MIT",
  "homepage": "https://github.com/your-org/n8n-nodes-wordpress-auto-tagger",
  "author": {
    "name": "WordPress Auto-Tagger Team",
    "email": "support@processfy.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/n8n-nodes-wordpress-auto-tagger.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=8.1"
  },
  "packageManager": "pnpm@8.6.2",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WordPressAutoTaggerApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/WordPressAutoTagger/WordPressAutoTagger.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}